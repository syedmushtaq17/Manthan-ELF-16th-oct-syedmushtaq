package com.testyantra.assess;

import java.util.function.Function;
import java.util.function.Predicate;

public class TestChips {

	public static void main(String[] args) {

	
		
		Chips c=new Chips();
		c.check();
		
		Function<Integer, Chips> a= c->{
			
			int x=c;
			
			switch (x) {
			case 1:
				return new Kurkure();
			

			case 2:
				
				
				return new Lays();
				
			case 3:
				
				
				return new Bingo();
				
		
			}
			
			
			
			
		};
	
		
		
	}

}
